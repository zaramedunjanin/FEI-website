<body class="body" id ="page-content">
<main>
    <div class="container content-border">
        <a href="/administrator/meetings/add">
            <button class="admin-btn add-btn">Add Meetings</button>
        </a>

        <table id = "table_id" class="table table-responsive table-hover db-table text-center">

            <tr>
                <th scope="col">ID</th>
                <th scope="col">Content</th>
                <th scope="col">Type</th>
                <th scope="col">Room ID</th>
                <th scope="col">Edit</th>
                <th scope="col">Delete</th>

            </tr>

            <%for(let i = 0; i < messages.length; i+=1){%>
            <tr id="row<%=messages[i].id%>">
                <td><%= messages[i].id%></td>
                <td><%= messages[i].content%></td>
                <td><%= messages[i].type%></td>
                <td><%= messages[i].room_id%></td>
                <td>
                    <a href="/administrator/messages/<%=messages[i].id%>/edit"><button name="edit_button" class="admin-btn" type="button">Edit</button></a>
                </td>
                <td>
                    <a href="/administrator/messages/<%=messages[i].id%>/delete"><button name="delete_button" class="admin-btn" type="button">Delete</button></a>
                </td>
            </tr>
            <%}%>

        </table>
    </div>
</main>
</body>
