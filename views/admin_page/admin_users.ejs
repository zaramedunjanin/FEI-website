<body class="body" id ="page-content">
<main>
    <div class="container content-border">
        <a href="/administrator/users/add">
            <button class="admin-btn add-btn">Add User</button>
        </a>
        <table id = "table_id" class="table table-hover db-table text-center">
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Username</th>
                <th scope="col">E-mail</th>

                <th scope="col">Edit</th>
                <th scope="col">Delete</th>

            </tr>

            <%for(let i = 0; i < users.length; i+=1){%>
            <tr id="row<%=users[i].id%>">
                <td><%= users[i].id%></td>
                <td><%= users[i].username%></td>
                <td><%= users[i].email%></td>
                <td>
                    <a href="/administrator/users/<%=users[i].id%>/edit"><button name="delete_button" class="admin-btn" type="button">Edit</button></a>
                </td>
                <td>
                    <a href="/administrator/users/<%=users[i].id%>/delete"><button name="delete_button" class="admin-btn" type="button">Delete</button></a>
                </td>
            </tr>
            <%}%>

        </table>
    </div>
</main>
</body>
